*
    list-style none 
    // height 100%
    // width 100%
.Category
    width 100%
    height 100%
    background #eeeeee
    .head
        text-align center
        color #ffffff
        font-size 18px
        line-height 50px
        font-weight bold
    
    .main
        display flex
        position absolute
        top 50px
        width 100%
        .left
            width 100px
            height 667px
            // background-color red
            position relative
            .leftMenu
                width 100%
                height 100%
                background-color #E6E6FA
                .menu
                    height 50px
                    width 100%
                    border 1px solid gray
                    line-height 60px
                    text-align center
                    font-size 12px
        .right
            flex 1
            .title
                height 35px
                text-align center
                line-height 35px
                background-color #00EEEE
                // margin-left 10px
            .content
                top 10px
                width 100%
                // height 500px
                background-color #BEBEBE
                display flex
                .img 
                    width 110px
                    height 100px
                    img
                        width 100%
                        height 100%
                .desc
                    flex 1
                    position relative
                    border 1px solid gray
                    .name
                        width 150px
                        height 30px
                        text-align center
                        line-height: 30px
                        overflow hidden
                        white-space nowrap
                        text-overflow ellipsis
                    .detail
                        
                        height 30px
                        width 150px
                        font-size: 12px
                        text-align center
                        border-bottom: 1px solid #fff
                        overflow hidden
                        white-space nowrap
                        text-overflow ellipsis
                    .price
                        height 40px
                        line-height 40px
                        text-align center

                        

