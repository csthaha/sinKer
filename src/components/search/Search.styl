.s
    position: fixed
    top: 0
    left: 0
    right: 0
    bottom: 0
    background-color: #fff
    z-index: 100
    width 100%
    height 100%
    animation-duration: 1s;
    animation-fill-mode: both;
    animation-name: fadeInLeft;
    .notFind
        background-color: #D8BFD8
        width: 330px
        margin-top: 100px   
        height: 50px
        line-height: 50px
        text-align: center
        font-size: 18px
        color: 	#2F4F4F
    .search-content
        width: 100%
        height: 100%
        .product
            display: flex
            // padding: 0 10px
            background-color: 	#FFFAFA
            margin: 10px 0
            .img
                max-width: 110px
                min-width: 110px 
                height: 100px
                img
                    width: 100%
                    height: 100%
            .message
                margin: 0 10px
                .message-name
                    height: 20px     
                    width: 255px
                    line-height: 20px
                    text-align: center
                    font-size: 18px
                .message-desc
                    height: 60px
                    // line-height: 60px
                    // overflow: hidden
                .message-price
                    // float: right
                    position: absolute
                    right: 30px
                    color: red
@keyframes fadeInLeft 
    from {
        opacity 0
        transform: translate3d(100%, 0, 0);
    }
    to {
        opacity 1
        transform: translate3d(0, 0, 0);
    }
