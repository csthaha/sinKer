.homeMenu
    position: fixed
    top: 0
    left: 0
    right: 0
    bottom: 0
    background-color: #fff
    z-index: 9998
    width 100%
    height 100%
    animation-duration: 1s;
    animation-fill-mode: both;
    animation-name: fadeMenu;
@keyframes fadeMenu 
    from {
        opacity 0
        // transform: translate3d(-100%, 0, 0);
        transform: translate3d(-100%,0,0);
    }
    to {
        opacity 1
        // transform: none 
        transform: translate3d(0,0,0);
    }